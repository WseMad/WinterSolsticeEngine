<?xml version="1.0" encoding="utf-8" ?>

<配置>

	<!-- JavaScript -->
	<js>
		<合集>
			<!--输出文件列表，若启用输出报告则输出到首个输出路径所在目录--> 
			<输出 路径="RunOpt/(10)WSE_nWse.js" ></输出>

			<!--输入目录（文件）列表，注意顺序，输出顺序将与此一致！--> 
			<输入 目录="F:/MyGit/WinterSolsticeEngine/WSE/nWse" 解析依赖="是" 压缩="是" 分离种子="是"></输入>
			<输入 目录="" >
				<文件 名称=""></文件>
			</输入>
		</合集>
		<合集>
			<!--<输出 路径="RunOpt/TestOpt.js" ></输出>-->
			<输入 目录="RunIpt" 解析依赖="否" 压缩="是"></输入>
			<输入 目录="" >
				<文件 名称=""></文件>
			</输入>
		</合集>
	</js>

	<!-- CSS -->
	<css>
		<合集>
			<!--<输出 路径="RunOpt/0Rset.css" ></输出>-->
			<输入 目录="" ></输入>
