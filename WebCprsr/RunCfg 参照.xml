	<!-- 宏（暂不支持） -->
	<宏>
		<!--<添加 名称="WSE_Dbg" 替换="1" />-->
	</宏>

	<!-- 启用：是，否 -->
	<压缩 启用="是">

		<!-- 启用：是（正在开发，请慎重），否 -->
		<!-- 保留：正则表达式，匹配的形参和局部变量名将保留，不匹配的将压缩；这里保留以“$”开头的标识符 -->
		<!-- 替换名生成：字母汉字（A-Z，a-z，所有汉字），序列号（从0开始递增） -->
		<!-- 序列号前缀：若选择序列号，则必须为替换名指定前缀，选择其他时忽略 -->
		<形参和局部变量名 启用="是" 保留="^\$" 替换名生成="字母汉字" 序列号前缀="序" >

			<!-- 启用：是，否 -->
			<!-- 保留：正则表达式，匹配的函数名将保留，不匹配的将压缩；这里保留构造函数名-->
			<局部函数名 启用="是" 保留="^[ou]?(?:n|[afis]?t)"  />

			<!-- 启用：是，否 -->
			<!--先不要用，还有BUG-->
			<属性访问 启用="是" />
		</形参和局部变量名>
	</压缩>
